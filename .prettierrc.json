{
  "importOrder": [
    "styles/reset.css",
    "styles/styles.scss",
    "(.*).css$",
    "(.*).scss$",
    "^[a-z](.*)",
    "^@(.*)",
    "./(.*)"
  ],
  "importOrderGroupNamespaceSpecifiers": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "quoteProps": "consistent",
  "singleAttributePerLine": true,
  "trailingComma": "es5"
}
